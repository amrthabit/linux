colorscheme blue
syntax enable
set number
set autoindent
filetype plugin indent on
set hlsearch
set cursorline
set swapfile
set tabstop=4
set shiftwidth=4
set expandtab
set mouse=a
set foldmethod=indent
set foldlevel=99
set laststatus=2
"set statusline=%<%f\ %h%m%r%=%-14.(%l,%c%V%)\ %P
set wildmenu
set wildmode=longest,list
set textwidth=80
